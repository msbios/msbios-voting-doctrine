{
  "name": "msbios/voting-doctrine",
  "description": "MSBios Voting Doctrine",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-voting-doctrine",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Voting\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Voting\\Doctrine\\": "tests/"
    }
  },
  "require": {
    "msbios/msbios": "~1.0",
    "msbios/voting": "~1.0",
    "msbios/voting-resource-doctrine": "~1.0"
  },
  "require-dev": {
    "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0",
    "msbios/application": "^1.0",
    "msbios/debug": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "alter": "./vendor/bin/doctrine-module orm:schema-tool:update --force",
    "themes": "cd bin && ln -s ../vendor/msbios/application/themes/default/public default",
    "serve": "php -S 0.0.0.0:3107 -t bin bin/msbios-voting-doctrine-module.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Voting\\Doctrine"
    }
  }
}
